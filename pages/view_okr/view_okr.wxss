.container-main-look{
  display: flex;
  flex-direction: column;
  min-height: 100%;
  background-color:#f5f5f5; 
}
.coatmain{
  padding:60rpx;
  background-color: #fff;
}
.headline{
  font-size:36rpx;
  font-family: "PingFang SC";
  color: rgd(0,0,0)
  line-height: 1.67;
  line-height: 50px;
}
.Object-item{
  margin-bottom:20rpx;
}
.text-content{
  background-color: #f2f8fc;
  padding-left:15rpx;
  height: 60px;
  line-height: 60px;
}
.text-content .objective-title{
  font-size: 32rpx;
  font-family: "PingFang SC";
  color: rgb(51,51,51);
}
.key-text-content{
  margin:20rpx;
  padding:20rpx;
  height: 45px;
  line-height: 45px;
  background-color: #f2f8fc;
}
.key-text-content .keyresult-title{
  font-size: 32rpx;
  font-family: "PingFang SC";
  color: rgb(51,51,51);
  background-color: #f2f8fc;
  height: 35rpx;
  line-height: 35rpx;

}
.decline-item .data-item{
  display: flex;
  justify-content: space-around;
  font-size: 32rpx;
  background-color: #f2f8fc;
  height: 35rpx;
  line-height: 35rpx;
}
.data-item .dataEnd{
  font-size: 22rpx;
  margin-left: 10rpx;
  line-height: 45rpx;
}
.dataGood{
  display: flex;
}
.dataGood .remainData{
  font-size: 22rpx;
  line-height: 45rpx;
}
.compile-btn{
  width: 86.7%;
  margin:0 auto;
}
.compile-btn .compile{
  font-size: 32rpx;
  width: 100%;
  background-color: #000;
  color: #fff;
  outline: none;
  height: 88rpx;
}

