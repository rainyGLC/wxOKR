.history-contact{
  display: flex;
  flex-direction: column;
  width: 100%;
  min-height: 100%;
  background-color: #fff;
}
.major-container{
 width: 100%;
}
.history_top_item{
  display: flex;
  justify-content: space-between;
  background-color: #a7cae4;
  height: 90rpx;
  line-height: 90rpx;
}
.nav-list-item-link{
  display: inline-block;
  vertical-align: top;
  text-align: center;
  line-height: 90rpx;
  width: 33%;
  font-size: 28rpx;
  margin:0;
  color:#000;
}
.nav-list-item-link:nth-child(2){
  background-image: url(data:image/png;base64,);
  background-size:cover;
  background-repeat: no-repeat;
}
.container{
  padding:15rpx;
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  padding-top:30rpx;
}
.container .time_top{
  width: 590rpx;
  height: 40rpx;
  font-size: 28rpx;
  color: #000000;
  margin:0 auto;
  text-align: center;
  background-color: #f2f8fc;
  font-weight: 200;
}
.todos-today{
  text-align: center;
  padding-top:60rpx;
}
.todos-title{
  font-size: 36rpx;
  color:#000000;
  font-weight: 600;
  text-align: left;
}
.todos-list{
  margin-top:60rpx;
}
.todos-list .todos-item{
  width: 590rpx;
  height: 110rpx;
  border-bottom: 1px solid #000;
  font-size: 0;
  position: relative;
}
.todo-work{
  display: inline-block;
  width: 472rpx;
  height: 100%;
  font-size: 28rpx;
  color:#b3b3b3;
  line-height: 110rpx;
  text-align: left;
  font-weight: 600;
  white-space: nowrap;
  overflow: hidden;
  padding-left: 50rpx;
}
.todos-click{
  vertical-align: top;
  display: inline-block;
  width: 118rpx;
  height: 100%;
  line-height: 220rpx;
  font-size: 28rpx;
  color: #333;
  text-align: center;
  font-weight: 600;
}
.todos-item:before{
  content: '';
  display: inline-block;
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  border-radius: 3px;
  border:1px solid #999;
  top:36rpx;
  left:0;
}
.todos-item:after{
  content: '';
  display: inline-block;
  width: 1px;
  height: 54rpx;
  background-color:#333;
  position: absolute;
  top:28rpx;
  right: 118rpx;
}
.todos-item.active:before{
  display: inline-block;
  border:none;
  width: 30rpx;
  height: 30rpx;
  background-image: url(data:image/png;base64,) no-repeat center center/contain;
}
.todos-item.active .todo-work{
  color: #333;
  text-decoration: line-through;
}
 .container_second{
  position: relative;
  margin-top: 15px;
  background-color: #ffffff;
 }
.second-tilte{
  font-size: 36rpx;
  text-align:left;
  margin-bottom: 15rpx;
  font-weight: 400;
  padding-left: 30rpx;
}
.second-surprise{
  font-size: 28rpx;
  color: #cccccc;
  font-weight: 200;
  padding-left: 80rpx;
}
.container_fourth{
  margin:0 auto;
  margin-top:15rpx;
  background-color: #ffffff;
}
.container_third{
  position: relative;
  margin:0 auto;
  margin-top: 15rpx;
}
.third-title{
  font-size: 36rpx;
  text-align: left;
  margin-bottom: 15rpx;
  font-weight: 400;
  padding-left: 30rpx;
}
.thired-reflect{
  font-size: 28rpx;
  font-weight: 200;
  color:#000000;
  padding-left: 80rpx;
}
.state-section{
  width: 100%;
  height: 100%;
  position: fixed;
  display: block;
  top:0;
  left: 0;
  background-color: rgba(0,0,0,0.5);
  z-index:999;
}
.state-container{
  display: block;
  position: absolute;
  bottom: 0;
  left:0;
  width: 100%;
  border-radius: 5px 5px 0 0;
  padding-top:49rpx;
}
.state-title{
  width: 100%;
  display: inline-block;
  font-size: 0;
  vertical-align: top;
}
.state-title-text{
  display: inline-block;
  font-size: 36rpx;
  font-weight: 500;
  line-height: 36rpx;
  margin-left: 30rpx;
  vertical-align: top;
  margin-bottom: 38rpx;
}
.state-map{
  display: inline-block;
  float: right;
  width: 38rpx;
  height: 38rpx;
  margin-right: 30rpx;
}
.state-desc{
  padding:0 30rpx;
}
.state-desc-text{
  font-size:24rpx;
  line-height: 30rpx;
  font-weight: 400;
  color: #000;
}
.state-button{
  height: 90rpx;
  display: block;
  width: 100%;
  line-height: 90rpx;
  text-align: center;
  color: #fff;
  background-color: #333;
  margin-top: 46rpx;
}





