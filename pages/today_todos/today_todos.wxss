.not-section{
  display: flex;
  min-height: 100%;
  width: 100%;
  flex-direction: column;
  background-color:#f7f7f7;
}
.todos-contaner{
  width: 100%;
  height: 100%;
}
.todos-section{
  background-color: #fff;
}
.nav-list{
  display: flex;
  justify-content: space-between;
  background-color: #a7cae4;
  height: 90rpx;
  line-height: 90rpx;
}
.nav-list-item-link{
  display: inline-block;
  vertical-align: top;
  text-align: center;
  line-height: 90rpx;
  width: 33%;
  font-size: 28rpx;
  margin:0;
  color:#000;
}
.nav-list-item-link:nth-child(1){
  background-image:url(data:image/png;base64,);
  background-size:cover;
  background-repeat: no-repeat;
}
.todos-content{
  padding:85rpx;
}
.todos-title{
  font-size: 36rpx;
  font-weight: 600;
  text-align: center;
  color:#000000;
  text-align: center;
  margin-top:44rpx;
  margin-bottom: 42rpx;
}
.todos-content-list{
  width: 590rpx;
}
.todos-content-list .todos-content-item{
  width: 100%;
  height: 112rpx;
  border-bottom: 1px solid #000000;
  font-size: 0;
  position:relative;
}
.todos-item-input{
  display: inline-block;
  vertical-align: top;
  height: 108rpx;
}
.todos-item-title{
  padding-top:20rpx;
  width: 472rpx;
  height: 100%;
  border:none;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #b3b3b3;
  outline: none;
}
.todos-click{
  vertical-align: top;
  display: inline-block;
  width: 118rpx;
  height: 108rpx;
  font-size: 28rpx;
  color:#000;
  text-align: center;
  padding-top:42rpx;
  position: relative;
}
.todos-content-item:before{
  content: '';
  display: inline-block;
  width: 2px;
  height: 54rpx;
  background-color: #000;
  position: absolute;
  right: 118rpx;
  top:27rpx;
}
.todos-happiness{
  padding:40rpx;
}
.happiness-title{
  font-size:36rpx;
  color: #000000;
  font-weight: 600;
  text-align: center;
  margin:38rpx 30rpx;
}
.happiness-inputbox{
  width: 590rpx;
  height: 159rpx;
}
.happiness-text{
  display: inline-block;
  width: 100%;
  height: 100%;
  border:none;
  outline: none;
  font-size: 28rpx;
  line-height: 28rpx;
  font-weight: 600;
  color:#b3b3b3;
  vertical-align: top;
  padding-top:20rpx;
  padding-left: 20rpx;
  background-color: #f2f8fc;
  border-radius: 5rpx;
  resize: none;
}
.todos-confirm{
  width: 650rpx;
  height: 88rpx;
  background-color: #000000;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
  font-weight: 600;
  line-height: 88rpx;
  border-radius: 10rpx;
  margin:30rpx 40rpx 83rpx 40rpx;
}
.todos-confirm-hint{
  text-align: center;
  font-size: 0;
  margin-bottom: 30rpx;
}
.todos-images{
  width: 25rpx;
  height: 25rpx;
  display: inline-block;
  margin-right: 10rpx;
  vertical-align: top;
}
.todos-text{
  display: inline-block;
  font-size: 24rpx;
  font-weight: 600;
  color: #ff1d25;
  vertical-align: top;
}
.shade-contanter{
  position: fixed;
  top:0;
  left: 0;
  right: 0;
  bottom: 0;
  animation: showmask .25s ease forwards;
  background-color: rgba(0,0,0,0.4);
  z-index:999;
}
.shade-contanter .shade-popup{
  width: 750rpx;
  background-color: rgba(0,0,0,.4);
  position: absolute;
  bottom: 0;
  left:0rpx;
}
.shade-blocklevel{
  background-color: #fff;
  border-top-left-radius: 25rpx;
  border-top-right-radius:25rpx;
}
.shade-topbar{
  height: 85rpx;
  background-color: #ffffff;
  border-radius: 20rpx 20rpx 0 0;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.shade-topbar .shade-title{
  display: inline-block;
  font-size: 36rpx;
  line-height: 85rpx;
  color: #000000;
  font-weight: 85rpx;
  vertical-align: top;
}
.shade-topbar-clone{
  float: right;
  width: 32rpx;
  height: 33rpx;
  margin-top:30rpx;
  margin-right: 30rpx;
}
.overflow{
  overflow: auto;
  display: inline-block;
  height: 800rpx;
  width: 750rpx;
}
.shade-content{
  margin-bottom: 20rpx; 
}
.overflow .shade-main-content{
  background-color: #fff;
  padding:30rpx 30rpx 0 30rpx;
}
.shade-content-title .shade-Chinese{
  display: inline-block;
  font-size: 28rpx;
  color: #333333;
  font-weight: 600;
  margin-left:18rpx;
  position: relative;
  height: 35rpx;
  line-height: 35rpx;
}
.shade-Chinese:before{
  content: '';
  display: inline-block;
  width: 6rpx;
  height: 26rpx;
  background-color: #000000;
  position: absolute;
  top:3rpx;
  left:-18rpx;
}
.shade-content-text{
  position:relative;
  height: 35rpx;
  line-height: 35rpx;
  border:1px solid #cccccc;
  border-radius: 7rpx;
  padding:26rpx 80rpx 20rpx 80rpx;
}
.shade-text-chinese{
  font-size: 24rpx;
  height: 35rpx;
  line-height: 35rpx;
  color:#666666;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.shade-text-english{
  display: none;
  font-size: 24rpx;
  color: #666666;
}
.shade-content-text.active{
  background-color: #999;
  border:1px solid #999;
}
.shade-content-text:before{
  content: '';
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  position: absolute;
  top:17rpx;
  left:20rpx;
}
.shade-content-text:after{
  content: '';
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border-left:2rpx solid #323232;
  border-bottom: 2rpx solid #323232;
  position: absolute;
  top:30rpx;
  right: 20rpx;
  transform: rotate(-45deg);
}
.gray{
  background-color: #999;
}
.gray .shade-text-chinese{
  color:#fff;
}
.shade-select{
  width: 100%;
  background-color: #fff;
}
.shade-select .shade-select-block{
  width: 690rpx;
}
.shade-select-block .shade-text-title{

}
.temporary-title{
  font-size: 28rpx;
  line-height: 28rpx;
  color:#333333;
  margin-top:22rpx;
}
.temporary-title{
  margin-top:12rpx;
  margin-bottom: 30rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #333333;
}
.confirm-button{
  width: 100%;
  height: 88rpx;
  background-color: #000000;
  text-align: center;
  font-size: 32rpx;
  font-weight: 600;
  color: #fff;
  line-height: 88rpx;
}












