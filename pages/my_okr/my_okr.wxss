.body-coat{
  display: flex;
  flex-direction: column;
  min-height: 100%;
  width: 100%;
  background-color:#f7f7f7;
}
.bodymain{
  background-color: #fff;
}
.bodymain .nav-list{
  display: flex;
  justify-content: space-between;
  border-radius: 5px 5px 0 0;
  background-color: #a7cae4;
  height: 89rpx;
  font-size: 0;
}
.nav-list-item{
  display: inline-block;
  text-align: center;
  height:90rpx;
  line-height:90rpx;
  width: 33%;
  font-size: 28rpx;
  margin:0;
  color: #333;
}
.nav-list-item:last-child{
  border-bottom: #fff;
  background-image:url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size:cover;
}
.bodymain-container{
  margin-top:20px;
  padding:0 30px;
}
.enddata-main{
  margin-bottom: 19rpx;
  font-size: 32rpx;
}
.enddata-main .end-tilte{
  display: inline-block;
}
.enddata-main .end-time{
  display: inline-block;
  font-size: 32rpx;
}
.object-text{
  margin-bottom: 10rpx;
  font-size:32rpx;
}
.object-content{
  margin-bottom: 10rpx;
  background-color:#f2f8fc;
  padding:16rpx 14rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.object-content .chinese-text{
  font-size: 32rpx;
}
.purple{
  position: relative;
}
.purple:before{
  content: '';
  position: absolute;
  left:-40rpx;
  top:10rpx;
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  border:5rpx solid #5f1783;
  z-index:1;
}
.add-message{
  display: inline-block;
  margin-top:20px;
}
.add-message .message-title{
  font-size: 30rpx;
}
.addbtn-main{
  width: 100%;
  text-align: center;
  padding-bottom: 30rpx;
}
.addbtn{
  border-radius: 6px;
  background-color: rgb(0,0,0);
  width: 650rpx;
  height: 88rpx;
  color:#fff;
  font-size: 32rpx;
  outline:none;
  border: none;
}


